        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sugo.takeout.mapper.RoleMapper">

    <resultMap id="BaseResultMap" type="com.sugo.takeout.bean.model.Role">
            <id property="id" column="id" jdbcType="INTEGER"/>
